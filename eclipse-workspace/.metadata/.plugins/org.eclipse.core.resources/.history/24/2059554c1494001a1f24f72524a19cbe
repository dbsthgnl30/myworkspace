package com.naver;

public class Apple {
	private String name;
	private int price;
	
	public Apple() {
		// TODO Auto-generated constructor stub
	}

	public Apple(String name, int price) {
		super();
		this.name = name;
		this.price = price;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		result = prime * result + price;
		return result;
	}

	@Override
	public String toString() {
		return "Apple [name=" + name + "]";
	}

	

	
	
	
	
	
	
	
}

